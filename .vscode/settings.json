{
  "[rocket-mvbasic]": {
    "editor.codeLens": false
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.rmv/catalog/**": true,
    "*/_*": false
  },
  "files.associations": {
    "*.embeddedhtml": "html",
    "**/.rmv/config/*.json": "jsonc"
  }
}
